        "node-sass": "^4.14.1",
        "sass-loader": "7.1.0",
        "ts-loader": "5.4.5",
        "typescript": "3.5.3",
        "webpack": "4.37.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "@fluentui/react-northstar": "^0.51.0",
